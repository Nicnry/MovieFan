@model List<Users>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h1>index</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Prénom</th>
                <th scope="col">Nom</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Users user in Model)
            {
            <tr>
                <th scope="row">@user.Id</th>
                <td>@user.Firstname</td>
                <td>@user.Lastname</td>
                <td><a href="/User/Details/@user.Id" class="btn btn-outline-primary" role="button" aria-pressed="true">Afficher le membre</a></td>
            </tr>
            }
        </tbody>
    </table>
</div>

